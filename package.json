{
  "name": "iput-kernel",
  "version": "1.0.0",
  "description": "IPUTでの大学生活を支援するためのアプリケーションシステム",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix --max-warnings 0 .",
    "formatter": "prettier --write .",
    "start": "node ./build/server.js",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev --name init && prisma generate",
    "prisma:studio": "prisma studio",
    "dev": "npm run prisma:migrate && (npm run prisma:studio &) && tsc && nodemon ./build/server.js",
    "type:check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint  --max-warnings=0 --fix"
  },
  "author": "bitcat",
  "license": "ISC",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@prisma/client": "^5.10.2",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.1",
    "@types/nodemailer": "^6.4.14",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.4.1",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "http-status": "^1.7.0",
    "ics": "^3.7.1",
    "influx": "^5.9.3",
    "influxdb-client": "^0.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "mongoose": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "punycode": "^2.3.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
